		System.out.println("  /\\  \t  /\\  \n /  \\ \t /  \\ \n/    \\ \t/    \\\n------\t------");
	}
	
	static void printCat() {
		
		System.out.println(" _ ");
		System.out.println("( \\");
		System.out.println(" ) )   _...._");
		System.out.println("( (  .        .  A.-.A");
		System.out.println(" \\ \\/          \\/ , , \\");
		System.out.println("  \\   \\        =;  t  /=");
		System.out.println("   \\    |''''.   `,--`");
		System.out.println("     / //    | ||");
		System.out.println("    /_,))    |_,))");
		
	}
	
	static void printUnicorn() {
		
		System.out.println("            /");
		System.out.println("        ,._/");
		System.out.println("       (((6\\");
		System.out.println("  _,---)  )r'");
		System.out.println(" (  ( )__))");
		System.out.println("  )//// \\" + "\\" + "\\");
		System.out.println("   \\" + "\\" + "\\  /\\" + "\\");
		System.out.println("   ,,,  , ,,");
		
	}
	
	static void printCastle() {
		
		System.out.println("   ||| || || || |                 |  |_| ||| || || || |");
		System.out.println("   |'' '' '' ''.|                 | _'-' |'' '' '' ''.|");
		System.out.println("   ;          .;;                 |'-'   ;          .;;");
		System.out.println("    \\-..____..;/  _  _  _  _  _  _| _  _'-\\-..____..;/");
		System.out.println("     ;--------;_.' || || || || || || || '.;;--------;");
		System.out.println("     |)     .;|;.  '' '' '' '' '' '' ''    | '-'  .;|");
		System.out.println("     |  .-. .(|;._     '-' ____      ___   |   .-.'-|");
		System.out.println("     |  | | .;|'--'_     .'____'.   '---'  |   | |.;|");
		System.out.println("     |  |_| .;|;.'--' ()/.| | |'.\\()  __   |   |_|.;|");
		System.out.println("     |  '-' .;|;.     |;;_|_|_|\\|;;  '--'  |  _'-'.;|");
		System.out.println("     | __   .;|;.     ;||-.-.-.-.|;        | '--' .;|");
		System.out.println("     |'--'  .;|;. _   ;||       |;|        |      .;|");
		System.out.println("     |      .;|;.'-' ; ||       |;|     _  |)     _;|");
		System.out.println("     |      '-|;.    ; ||       ;||    '-' |     '--|");
		System.out.println("     |   _  .;|).   ;  ||       ;||)       |   _  .;|");
		System.out.println("     |  '-' .;|;.  ;  (||      :|||        |  '-' .;|");
		System.out.println(" .'. ;._____.;;-- ;---->'-.--.;-'<'--------;.-----.;;.'.");
		System.out.println("((  (          )_;___.' .' .  : //________(          ) ))");
		System.out.println(" '._ '--------' : -.' ' . ' '; //'        _ '--------'.'");
		System.out.println("      __  .--'  ;,' ,'     '; //     -.._   __  _.-  -");
		System.out.println("  ._   __     _;. .  .   .   /_  -.       ---      ");
		System.out.println("            /-:._._._._._._(/:-\\  .     ..    __     ");
		System.out.println("   _.-''._.__._._._._._._._..__._. ',_.,.  .,..  '-55t-");
		
