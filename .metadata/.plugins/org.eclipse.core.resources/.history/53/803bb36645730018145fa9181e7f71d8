public class Main {
	
	public static void main(String[] args) {
		printSquare();
		printRectangle();
		printCircle();
		printTriangle();
		printCat();
		System.out.println();
		printUnicorn();
		System.out.println();
		printCastle();
	}
	
	static void printSquare() {
		
		//Hard-coded part
		/*System.out.println("*********\t*********");
		System.out.println("*\t*\t*\t*");
		System.out.println("*\t*\t*\t*");
		System.out.println("*\t*\t*\t*");
		System.out.println("*********\t*********");*/
		
		//All in one line.
		System.out.println("*********\t*********\n*\t*\t*\t*\n*\t*\t*\t*\n*\t*\t*\t*\n*********\t*********");
		
	}
	
	static void printRectangle() {
		
		//Hard-coded part
		/*System.out.println(" _______ \t _______ ");
		System.out.println("|\t|\t|\t|");
		System.out.println("|\t|\t|\t|");
		System.out.println("|\t|\t|\t|");
		System.out.println("|\t|\t|\t|");
		System.out.println("|\t|\t|\t|");
		System.out.println("|_______|\t|_______|");*/
		
		//All in one line
		System.out.println(" _______ \t _______ \n|\t|\t|\t|\n|\t|\t|\t|\n|\t|\t|\t|\n|\t|\t|\t|\n|\t|\t|\t|\n|_______|\t|_______|");
		
	}
	
	static void printCircle() {
		
		//Hard-coded part
		/*System.out.println("  ***  \t  ***  ");
		System.out.println(" *   * \t *   * ");
		System.out.println("*     *\t*     *");
		System.out.println(" *   * \t *   * ");
		System.out.println("  ***  \t  ***  ");*/
		
		//All in one line
		System.out.println("  ***  \t  ***  \n *   * \t *   * \n*     *\t*     *\n *   * \t *   * \n  ***  \t  ***  ");
	}
	
	static void printTriangle() {
		
		//Hard-coded part
		/*System.out.println("  /\\  \t  /\\  ");
		System.out.println(" /  \\ \t /  \\ ");
		System.out.println("/    \\ \t/    \\");
		System.out.println("------\t------");*/
		
		//All in one line
		System.out.println("  /\\  \t  /\\  \n /  \\ \t /  \\ \n/    \\ \t/    \\\n------\t------");
	}
	
	static void printCat() {
		
		System.out.println(" _ ");
		System.out.println("( \\");
		System.out.println(" ) )   _...._");
		System.out.println("( (  .        .  A.-.A");
		System.out.println(" \\ \\/          \\/ , , \\");
		System.out.println("  \\   \\        =;  t  /=");
		System.out.println("   \\    |''''.   `,--`");
		System.out.println("     / //    | ||");
		System.out.println("    /_,))    |_,))");
		
	}
	
	static void printUnicorn() {
		
		System.out.println("            /");
		System.out.println("        ,._/");
		System.out.println("       (((6\\");
		System.out.println("  _,---)  )r'");
		System.out.println(" (  ( )__))");
		System.out.println("  )//// \\" + "\\" + "\\");
		System.out.println("   \\" + "\\" + "\\  /\\" + "\\");
		System.out.println("   ,,,  , ,,");
		
	}
	
	static void printCastle() {
		
		System.out.println("{{  {          }_;___.' .' .  : //________(          ) ))");
		System.out.println(" '._ '--------' : -.' ' . ' '; //'        _ '--------'.'");
		
	}
}
