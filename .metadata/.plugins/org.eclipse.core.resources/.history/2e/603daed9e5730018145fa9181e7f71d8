
public class Main {

	public static void main(String[] args) {
		final int power = 5;
		System.out.println("\t Power");
		for(int i = 0; i <= 5; i++) {
			if(i % 6 == 0) {
				System.out.println("N " + i + getPower(i, power));
			}
		}
	}
	
	static int getPower(int x, int y) {
		for(int i = 0; i < y; i++) {
			x *= x;
			System.out.print("   " + x + "   ");
		}
		return x;
	}
	
}
